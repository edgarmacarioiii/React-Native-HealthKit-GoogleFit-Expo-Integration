{
  "expo": {
    "name": "healthkit-google-fit-sample-integration",
    "slug": "healthkit-google-fit-sample-integration",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.healthkit.googlefit.integration"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.test.healthkit.googlefit.integration"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [["react-native-health"], ["react-native-google-fit"]]
  }
}
